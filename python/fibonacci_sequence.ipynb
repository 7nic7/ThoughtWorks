    "def fib(n, l=[1, 1]):\n",
    "    if n<=2:\n",
    "        if n==1:\n",
    "            return l[0]\n",
    "        else:\n",
    "            return l\n",
    "    \n",
    "    return fib(n-1, l +[l[-1] + l[-2]])\n",
    "    "
   ]
